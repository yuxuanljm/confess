.main{
  padding:10px
}
.logo{
  border:1px black solid;
  display: flex;
  height: 40px;
  justify-content: space-between;
  align-items: center;
}
.logo .left{
  width:50px;
  height: 30px;
}
.content{
  width:100%;
  height:150px;
  border:1px black solid;
  margin:20px 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  /* box-sizing: border-box; */
}
.biaobai{
  width:100%;
  height:150px;
  border-radius: 2%;
  /* border:1px black solid; */
}
.limian{
  display: flex;
  justify-content: space-around;
  align-items: center;
  
}
.limian .circle{
  border:1px black solid;
  width:80px;
  height:80px;
  margin-top:10%;
  border-radius: 50%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}





